{
  "name": "test-commands-server",
  "version": "1.0.0",
  "description": "MCP server created with basic template",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "doctor": "tsx src/scripts/doctor.ts",
    "lint": "eslint src --ext .ts",
    "commands:validate": "tsx src/scripts/validate-commands.ts",
    "commands:submit": "echo 'Submitting to Commands.com marketplace...' && echo 'Visit https://commands.com/submit for manual submission'"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "commands",
    "ai-tools"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.4",
    "eslint": "^9.15.0",
    "@typescript-eslint/parser": "^8.16.0",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
